<Window x:Class="OpenFSRC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenFSRC"
        mc:Ignorable="d"
        Title="OpenFSRC" Width="300" Height="220" MinWidth="300" MinHeight="220" Closing="Window_Closing" Initialized="Window_Initialized">
    <Grid>
        <Button x:Name="buttonConnect" Content="Start" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="buttonConnect_Click"/>
        <Button x:Name="buttonDisconnect" Content="Stop" HorizontalAlignment="Left" Margin="46,10,0,0" VerticalAlignment="Top" Click="buttonDisconnect_Click"/>
    </Grid>
</Window>
